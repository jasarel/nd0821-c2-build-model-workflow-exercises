name: exercise_5
conda_env: conda.yml

entry_points:
  main:
    parameters:
      input_artifact:
        description: Name for the atifact being used 
        type: str
      artifact_name:
        description: Name of the W&B artifact being created
        type: str
      artifact_type: 
        description: Type of the artifact being created
        type: str
      artifact_description:
        description: Desciption for the artifact
        type: str
    command: >-
      python run.py --input_artifact {input_artifact} \
                              --artifact_name {artifact_name} \
                              --artifact_type {artifact_type} \
                              --artifact_description {artifact_description}
